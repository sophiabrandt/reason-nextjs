    "next": "9.1.1",
    "next-compose-plugins": "^2.2.0",
    "node-sass": "^4.12.0",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "reason-react": "0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "babel-plugin-bucklescript": "^0.5.3",
    "bs-platform": "5.1.0",
    "concurrently": "^5.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "next-transpile-modules": "^2.3.1",
    "prettier": "^1.18.2",
    "prettier-eslint-cli": "^5.0.0"
